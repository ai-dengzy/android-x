<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item  android:state_pressed="true" >
    <shape>
      <solid android:color="#A1A1A1"/>
      <corners android:radius="@dimen/general_20"/>
      <gradient android:startColor="#A1A1A1" android:endColor="#ffffff" android:type="linear"/>
    </shape>
  </item>
  <item android:state_pressed="false">
    <shape>
      <stroke android:width="@dimen/general_1"
              android:color="#4ca0fe"/>
      <corners android:radius="@dimen/general_20"/>
      <gradient android:startColor="#FF0CB40C" android:endColor="#ffffff" android:type="linear"/>

    </shape>
  </item>
</selector>