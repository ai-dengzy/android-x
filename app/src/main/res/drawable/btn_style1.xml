<?xml version="1.0" encoding="utf-8"?>
<!--
    round button
    stroke color:gray(width:1dp)
    background color:white
-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true">
    <shape>
      <solid android:color="#A1A1A1"/>
      <corners android:radius="@dimen/general_20"/>
      <gradient android:startColor="#A1A1A1" android:endColor="#ffffff" android:type="linear"/>
    </shape>
  </item>
  <item android:state_pressed="false">
    <shape>
      <corners android:radius="@dimen/general_30"/>
      <stroke android:color="@color/gray" android:width="@dimen/general_1"/>
      <gradient android:startColor="#E5E5E5E5" android:endColor="#ffffff" android:type="linear"/>
    </shape>
  </item>
</selector>