<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="@dimen/general_80"
    android:layout_height="@dimen/general_80"

    >
    <ImageView
        android:id="@+id/iv_image"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="@dimen/general_5"
        android:scaleType="center"
        />
    <ImageView
        android:visibility="visible"
        android:id="@+id/iv_delete"
        android:layout_width="@dimen/general_10"
        android:layout_alignParentRight="true"
        android:src="@drawable/delete_round"
        android:layout_height="@dimen/general_10" />
</RelativeLayout>