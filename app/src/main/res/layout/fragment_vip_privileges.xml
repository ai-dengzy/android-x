<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <View
        android:id="@+id/view_vip_privileges"
        android:layout_width="match_parent"
        android:layout_height="@dimen/general_37"
        android:background="@color/colorPrimaryDark"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/tv_vip_privileges_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/ic_back_xml"
        android:layout_marginStart="8dp"
        android:layout_marginTop="@dimen/top_4"
        app:layout_constraintBottom_toBottomOf="@+id/view_vip_privileges"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view_vip_privileges"/>

    <TextView
        android:id="@+id/tv_vip_privileges_title"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/general_30"
        android:text="会员特权"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="@dimen/font_tv_normal_16"
        app:layout_constraintTop_toTopOf="@id/view_vip_privileges"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/view_vip_privileges"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_vip1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/general_80"
        android:background="@drawable/vip1"
        android:layout_marginTop="@dimen/top_20"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_vip_privileges" />
    <TextView
        android:id="@+id/tv_vip2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/general_80"
        android:background="@drawable/vip2"
        android:layout_marginTop="@dimen/top_20"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_vip1"/>
    <TextView
        android:id="@+id/tv_vip3"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/general_80"
        android:background="@drawable/vip3"
        android:layout_marginTop="@dimen/top_20"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_vip2"/>
</androidx.constraintlayout.widget.ConstraintLayout>